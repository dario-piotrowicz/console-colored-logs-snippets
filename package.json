{
  "name": "console-colored-logs",
  "displayName": "Console Colored Logs Snippets",
  "description": "Minimal set of snippets for adding colors to console.logs",
  "version": "0.0.5",
  "publisher": "dariopiotrowicz",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Snippets",
    "Debuggers"
  ],
  "markdown": "github",
  "keywords": [
    "console.log",
    "javascript",
    "typescript",
    "nodejs"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets",
        "extensions": [".js", ".jsx", ".cjs", ".mjs"]
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.code-snippets",
        "extensions": [".ts", ".tsx"]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dario-piotrowicz/console-colored-logs-snippets.git"
  },
  "icon": "logo.png",
  "scripts": {
    "build": "ts-node generate-snippets-file.ts",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
